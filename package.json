{
    "private": true,
    "packageManager": "pnpm@7.28.0",
    "scripts": {
        "build": "vite build",
        "dev": "vite --port 3333",
        "lint": "eslint .",
        "typecheck": "vue-tsc --noEmit",
        "preview": "vite preview",
        "test": "vitest",
        "up": "taze major -I",
        "postinstall": "npx simple-git-hooks"
    },
    "dependencies": {
        "@vueuse/core": "^9.13.0",
        "vanilla-tilt": "^1.8.0",
        "vue": "^3.2.47",
        "vue-router": "^4.1.6"
    },
    "devDependencies": {
        "@antfu/eslint-config": "^0.36.0",
        "@iconify-json/carbon": "^1.1.16",
        "@types/node": "^18.14.6",
        "@vitejs/plugin-vue": "^4.0.0",
        "@vue-macros/volar": "^0.8.4",
        "@vue/test-utils": "^2.3.0",
        "eslint": "^8.35.0",
        "jsdom": "^21.1.0",
        "lint-staged": "^13.1.2",
        "pnpm": "^7.28.0",
        "prettier": "2.8.7",
        "sass": "^1.60.0",
        "simple-git-hooks": "^2.8.1",
        "taze": "^0.9.0",
        "typescript": "^4.9.5",
        "unplugin-auto-import": "^0.15.0",
        "unplugin-vue-components": "^0.24.0",
        "unplugin-vue-macros": "^1.9.1",
        "vite": "^4.1.4",
        "vite-plugin-pages": "^0.28.0",
        "vitest": "^0.29.2",
        "vue-tsc": "^1.2.0"
    },
    "simple-git-hooks": {
        "pre-commit": "pnpm lint-staged"
    },
    "lint-staged": {
        "*": "eslint --fix"
    },
    "eslintConfig": {
        "extends": "@antfu"
    }
}
